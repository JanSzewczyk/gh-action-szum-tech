    "eslint": "^8.22.0",
    "eslint-import-resolver-typescript": "^3.2.5",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^26.8.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
